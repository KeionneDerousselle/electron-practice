{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch",
            "type": "node",
            "request": "launch",
            "program": "${workspaceRoot}\\ipc-demo\\src\\main.js",
            "stopOnEntry": false,
            "args": [],
            "cwd": "${workspaceRoot}\\ipc-demo",
            "preLaunchTask": null,
            "runtimeExecutable": "${workspaceRoot}\\ipc-demo\\node_modules\\.bin\\electron",
            "runtimeArgs": [
                ".",
                "--enable-logging"
            ],
            "env": {
                "NODE_ENV": "development"
            },
            "console": "externalTerminal",
            "sourceMaps": false,
            "outDir": null
        },
        {
            "name": "Attach",
            "type": "node",
            "request": "attach",
            "port": 5858,
            "address": "localhost",
            "restart": false,
            "sourceMaps": false,
            "outDir": null,
            "localRoot": "${workspaceRoot}",
            "remoteRoot": null
        },
        {
            "name": "Attach to Process",
            "type": "node",
            "request": "attach",
            "processId": "${command.PickProcess}",
            "port": 5858,
            "sourceMaps": false,
            "outDir": null
        }
    ]
}